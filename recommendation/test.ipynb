    "import time\n",
    "import datetime\n",
    "\n",
    "def time_to_datetime(input: int):\n",
    "    t = time.gmtime(input)\n",
    "    time_tuple = (t.tm_year, t.tm_mon, t.tm_hour)\n",
    "    \n",
    "    return datetime.datetime(*time_tuple, 0, 0)\n",
    "\n",
    "\n",
    "print(type(time_to_datetime(1260759131)))\n",
    "print(time_to_datetime(1260759131))"
